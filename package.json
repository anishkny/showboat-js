{
  "name": "showboat",
  "version": "0.4.0",
  "description": "Create executable demo documents that show and prove an agent's work",
  "main": "index.js",
  "bin": {
    "showboat": "./bin/showboat.js"
  },
  "scripts": {
    "postinstall": "node install.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "documentation",
    "demo",
    "markdown",
    "executable",
    "agent",
    "proof-of-work"
  ],
  "author": "Simon Willison (https://github.com/simonw/showboat)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/anishkny/showboat-js.git"
  },
  "bugs": {
    "url": "https://github.com/anishkny/showboat-js/issues"
  },
  "homepage": "https://github.com/anishkny/showboat-js#readme",
  "engines": {
    "node": ">=14"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "adm-zip": "^0.5.10",
    "tar": "^7.5.7"
  },
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "*.js",
      "!install.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
